{
  "build": {
    "arduino": {
      "ldscript": "esp32_out.ld"
    },
    "core": "esp32",
    "extra_flags": ["-DBOARD_HAS_PSRAM", "-DARDUINO_ESP32_DEV", "-mfix-esp32-psram-cache-issue",
      "-mfix-esp32-psram-cache-strategy=memw"],
    "f_cpu": "240000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",
    "mcu": "esp32",
    "variant": "wroverb-board"
  },
  "connectivity": ["wifi", "bluetooth"],
  "debug": {
    "openocd_board": "esp-wroom-32.cfg"
  },
  "frameworks": ["arduino", "espidf"],
  "name": "wroverb-board",
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 532480,
    "maximum_size": 4194304,
    "protocols": ["esptool"],
    "require_upload_port": true,
    "speed": 460800
  },
  "url": "https://www.ttgo.com",
  "vendor": "TTGO"
